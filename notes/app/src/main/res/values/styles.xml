        <item name="colorAccent">#9e9e9e</item>
    </style>

    <style name="TileAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#009688</item>
        <item name="colorPrimaryDark">#00796b</item>
        <item name="colorAccent">#9e9e9e</item>
    </style>

    <style name="GreenAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#4caf50</item>
        <item name="colorPrimaryDark">#388e3c</item>
        <item name="colorAccent">#8bc34a</item>
    </style>

    <style name="IndigoAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#3f51b5</item>
        <item name="colorPrimaryDark">#303f9f</item>
